{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "no-var-requires": false,
    "interface-name": false,
    "no-unused-expression": false,
    "object-literal-sort-keys": false,
    "variable-name": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
